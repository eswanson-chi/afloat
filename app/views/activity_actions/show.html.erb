<div class="page-header">
  <h3>Activity Action #<%= @activity_action.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Activity</dt>
      <dd><%= @activity_action.activity_id %></dd>

      <dt>Action</dt>
      <dd><%= @activity_action.action_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/activity_actions" class="btn btn-primary">
        Back
      </a>
      <a href="/activity_actions/<%= @activity_action.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_activity_action/<%= @activity_action.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
