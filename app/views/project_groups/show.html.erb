<div class="page-header">
  <h3>Project Group #<%= @project_group.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Project</dt>
      <dd><%= @project_group.project_id %></dd>

      <dt>Group</dt>
      <dd><%= @project_group.group_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/project_groups" class="btn btn-primary">
        Back
      </a>
      <a href="/project_groups/<%= @project_group.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_project_group/<%= @project_group.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
