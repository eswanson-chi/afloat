
<div class="row">
  <div class="list-group">
    <div class="col-md-9">
    <table class="table table-striped table-hover">
      <tr>
        <th>Task</th>
        <th>My app</th>
        <th>Group user</th>
        <th>Project group</th>
      </tr>

        <% @events.each do |event| %>
        <tr>
          <td><%= event.task_id %></td>
          <td><%= event.my_app_id %></td>
          <td><%= event.group_user_id %></td>
          <td><%= event.project_group_id %></td>
        </tr>
      </div>
      <% end %>
    </table>
  </div>
</div>
