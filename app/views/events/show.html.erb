<div class="page-header">
  <h3>Event #<%= @event.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Task</dt>
      <dd><%= @event.task_id %></dd>

      <dt>My app</dt>
      <dd><%= @event.my_app_id %></dd>

      <dt>Group user</dt>
      <dd><%= @event.group_user_id %></dd>

      <dt>Project group</dt>
      <dd><%= @event.project_group_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/events" class="btn btn-primary">
        Back
      </a>
      <a href="/events/<%= @event.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_event/<%= @event.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
